{
  "name": "geodesy-babel",
  "description": "Libraries of geodesy functions",
  "homepage": "http://www.movable-type.co.uk/scripts/geodesy-library.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisveness/geodesy"
  },
  "keywords": [
    "geodesy",
    "latitude",
    "longitude",
    "distance",
    "bearing",
    "destination",
    "haversine",
    "vincenty",
    "rhumb",
    "ellipsoid",
    "datum",
    "reference frame",
    "TRF",
    "wgs84",
    "utm",
    "mgrs"
  ],
  "author": "Chris Veness",
  "version": "2.2.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "bugs": "https://github.com/chrisveness/geodesy/issues",
  "scripts": {
    "lint": "./node_modules/.bin/eslint -c .eslintrc lib/**",
    "build": "mkdir src && cp ./node_modules/geodesy/*.js ./src && ./node_modules/.bin/babel src --out-dir lib && npm run lint"
  },
  "devDependencies": {
    "@babel/cli": "7.23.9",
    "@babel/core": "7.23.9",
    "@babel/node": "7.23.9",
    "@babel/plugin-transform-runtime": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/eslint-parser": "7.23.9",
   "eslint": "^8.56.0"
  },
  "files": [
    "lib/*"
  ],
  "jsdoc": {
    "plugins": [
      "plugins/markdown"
    ],
    "markdown": {
      "idInHeadings": true
    }
  },
  "dependencies": {
    "@babel/runtime": "7.23.9",
    "geodesy": "^2.2.0"
  }
}
